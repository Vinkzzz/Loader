-- Lib

local MangoLib = loadstring(game:HttpGet("https://pastebin.com/raw/N3sKKx0a"))()

spawn(function()
MangoLib:Notify("System", "Setting Things Up!")
end)

---------------------------------------------------------------------------------------------------

-- Tabs

---------------------------------------------------------------------------------------------------

local Tab = MangoLib:Tab("Main")

local Tab1 = MangoLib:Tab("Emotes")

local Tab2 = MangoLib:Tab("Settiings")

---------------------------------------------------------------------------------------------------

-- Main



Tab2:Slider("Walk Speed", 16, 500, 40, function(t)
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = t
end)

Tab2:Slider("JumpPower", 16, 500, 40, function(t)
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = t
end)


Tab2:Slider("Gravity",0,500,196, function(t)
    workspace.Gravity = t
end)

---------------------------------------------------------------------------------------------------

