y-- // Booting Library \\ --
local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
 
-- // Extra \\ --
game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "Zintux | Key System",
            Text = "Key System has been Loaded"
        })
 
-- // Functions \\ --
function CorrectKey()
    if game.PlaceId == nil then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/GeronimoZhidkova/Blood-Hub/main/Ninja%20Legends%20%7C%20BH%20V1"))()
    end
end
 
local colors = {
    SchemeColor = Color3.fromRGB(80,0,0),
    Background = Color3.fromRGB(188,0,0),
    Header = Color3.fromRGB(110, 0, 0),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(110, 0, 0)
}
 
 
_G.Key = "Zintux"
_G.KeyInput = "string"
 
 
-- // Creating Window \\ --
local Window = Library.CreateLib("Blood Hub | Key System", colors)
 
-- // Define Tabs \\ --
local Tab1 = Window:NewTab("Key System")
local Tab2 = Window:NewTab("More")
 
-- // Define Tab1 \\ --
local Section1 = Tab1:NewSection("Key")
 
Section1:NewTextBox("Enter Key", "Enter the key", function(Value)
    _G.KeyInput = Value
end)
 
Section1:NewButton("Check Key", "Check the key enter", function()
    if _G.KeyInput == _G.Key then
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "Key System",
            Text = "You've entered the correct key!"
        })
        CorrectKey()
    else
        game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "Key System",
            Text = "Wrong Key!"
        })
    end
end)
 
-- // Define Tab2  \\ --
local Section2 = Tab2:NewSection("Information")
 
Section2:NewButton(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name, "Shows Current Game", function()
    setclipboard(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name)    
end)
 
Section2:NewButton("Copy Discord Tag","Copies My Discord Tag", function()
    setclipboard("!  >if0undy0u#0000")
end)
